{
	"version": "0.0.8",
	"name": "svelte-navbar",
	"description": "A cool animated navbar component for Svelte that switches between a side and a bottom navbar in a very funky way",
	"keywords": [
		"funky",
		"blobby",
		"svelte",
		"navbar",
		"component",
		"animated",
		"responsive"
	],
	"homepage": "https://github.com/HannesGitH/svelte-navbar#readme",
	"author": "hannes Hattenbach <hanneshattenbach.dev@gmail.com> (https://hannesgith.github.io)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/HannesGitH/svelte-navbar/issues",
		"email": "hanneshattenbach.dev+sveltenavbarbug@gmail.com"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"@material/button": "^14.0.0",
		"flubber2": "^0.4.9",
		"sass": "^1.55.0",
		"svelte-scrolling-plus": "^1.2.3"
	}
}
